<?xml version="1.0" encoding="utf-8"?>
<com.jianfanjia.cn.view.scrollview.ReboundScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_layout_bg"
    android:focusable="false"
    android:scrollbars="none">

    <LinearLayout
        android:id="@+id/contentLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical"
        android:paddingBottom="@dimen/space_100"
        android:paddingTop="@dimen/space_10">

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <RelativeLayout
            android:id="@+id/act_edit_req_city"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:nextFocusDown="@+id/act_edit_req_street_content">

            <requestFocus />

            <TextView
                android:id="@+id/act_edit_req_city_title"
                style="@style/style_edit_req_title"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/str_city" />


            <ImageView
                android:id="@+id/act_edit_req_city_goto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_next" />

            <TextView
                android:id="@+id/act_edit_req_city_content"
                style="@style/style_edit_req_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp"
                android:ellipsize="end"
                android:singleLine="true" />
        </RelativeLayout>

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <LinearLayout
            android:id="@+id/act_edit_req_cell"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingRight="24dp">

            <TextView
                android:id="@+id/act_edit_req_cell_title"
                style="@style/style_edit_req_title"
                android:text="@string/str_cell" />

            <EditText
                android:id="@+id/act_edit_req_cell_content"
                style="@style/style_edit_req_edit"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center_vertical|right"
                android:hint="@string/example_input_cell"
                android:maxLength="20"
                android:nextFocusDown="@+id/act_edit_req_dong_content" />

        </LinearLayout>

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <LinearLayout
            android:id="@+id/act_edit_req_adressdetail"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingRight="24dp">

            <TextView
                android:id="@+id/act_edit_req_dong_title"
                style="@style/style_edit_req_title"
                android:text="@string/house_number_text" />

            <EditText
                android:id="@+id/act_edit_req_dong_content"
                style="@style/style_edit_req_edit"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center_vertical|right"
                android:hint="@string/example_input_dong"
                android:maxLength="20" />

        </LinearLayout>

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <View
            style="@style/divier_line"
            android:layout_marginTop="@dimen/space_10"
            android:background="@color/horzontal_line_color" />


        <RelativeLayout
            android:id="@+id/act_edit_req_housetype"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <TextView
                android:id="@+id/act_edit_req_housetype_title"
                style="@style/style_edit_req_title"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/str_housetype" />


            <ImageView
                android:id="@+id/act_edit_req_housetype_goto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_next" />

            <TextView
                android:id="@+id/act_edit_req_housetype_content"
                style="@style/style_edit_req_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp" />
        </RelativeLayout>

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />


        <RelativeLayout
            android:id="@+id/act_edit_req_work_type"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <TextView
                android:id="@+id/act_edit_req_work_type_title"
                style="@style/style_edit_req_title"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/str_work_type" />


            <ImageView
                android:id="@+id/act_edit_req_work_type_goto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_next" />

            <TextView
                android:id="@+id/act_edit_req_work_type_content"
                style="@style/style_edit_req_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp" />
        </RelativeLayout>


        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <RelativeLayout
            android:id="@+id/act_edit_req_housearea"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <TextView
                android:id="@+id/act_edit_req_housearea_title"
                style="@style/style_edit_req_title"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/str_coveredarea" />

            <EditText
                android:id="@+id/act_edit_req_housearea_content"
                style="@style/style_edit_req_edit"
                android:layout_alignParentRight="true"
                android:layout_toRightOf="@+id/act_edit_req_housearea_title"
                android:gravity="center_vertical|right"
                android:inputType="number"
                android:maxLength="4"
                android:nextFocusDown="@+id/act_edit_req_decoratebudget_content"
                android:numeric="integer" />

        </RelativeLayout>


        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <RelativeLayout
            android:id="@+id/act_edit_req_decoratebudget"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <TextView
                android:id="@+id/act_edit_req_decoratebudget_title"
                style="@style/style_edit_req_title"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/str_decoratebudget_with_unit" />

            <EditText
                android:id="@+id/act_edit_req_decoratebudget_content"
                style="@style/style_edit_req_edit"
                android:layout_alignParentRight="true"
                android:layout_toLeftOf="@+id/act_edit_req_decoratebudget_unit"
                android:gravity="center_vertical|right"
                android:hint="@string/example_input_budget"
                android:inputType="number"
                android:maxLength="3"
                android:minWidth="@dimen/space_48"
                android:numeric="integer" />

        </RelativeLayout>

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <LinearLayout
            android:id="@+id/act_edit_req_decoratebudget_365"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="@dimen/space_8"
            android:paddingTop="@dimen/space_8"
            android:visibility="gone">

            <TextView
                android:id="@+id/act_edit_req_decoratebudget_365_alert"
                style="@style/listview_item_text_style_small"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="@dimen/space_8"
                android:text="@string/decoratebudget_365_alert"
                android:textColor="@color/color_red"
                android:visibility="gone" />

            <TextView
                android:id="@+id/act_edit_req_decoratebudget_365_title"
                style="@style/listview_item_text_style_small"
                android:layout_gravity="center_horizontal"
                android:text="@string/decoratebudget_365_title" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/space_8"
                android:layout_marginTop="@dimen/space_8"
                android:orientation="horizontal"
                android:weightSum="3">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@mipmap/bg_req_budget"
                    android:orientation="vertical"
                    android:weightSum="2">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="2">

                        <LinearLayout
                            android:id="@+id/decoratebudget_365_basic_price_layout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/decoratebudget_365_basic_price"
                                style="@style/style_edit_req_title"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:maxLength="5"
                                android:singleLine="true"
                                android:textColor="@color/black_color"
                                android:textSize="@dimen/big_size"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/decoratebudget_365_basic_unit"
                                style="@style/style_edit_req_title"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:text="@string/unit_million"
                                android:textSize="@dimen/small_size" />

                        </LinearLayout>

                    </RelativeLayout>

                    <TextView
                        android:id="@+id/decoratebudget_365_basic"
                        style="@style/listview_item_text_style_small"
                        android:layout_height="0dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:paddingBottom="@dimen/space_2"
                        android:text="@string/decoratebudget_365_basic"
                        android:textColor="@color/font_white" />

                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/space_8"
                    android:src="@mipmap/icon_plus" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@mipmap/bg_req_budget"
                    android:orientation="vertical"
                    android:weightSum="2">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="2">

                        <LinearLayout
                            android:id="@+id/decoratebudget_365_individuation_price_layout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:orientation="horizontal"
                            android:visibility="gone">

                            <TextView
                                android:id="@+id/decoratebudget_365_individuation_price"
                                style="@style/style_edit_req_title"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:maxLength="5"
                                android:singleLine="true"
                                android:textColor="@color/black_color"
                                android:textSize="@dimen/big_size"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/decoratebudget_365_individuation_unit"
                                style="@style/style_edit_req_title"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:text="@string/unit_million"
                                android:textSize="@dimen/small_size" />

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/decoratebudget_365_individuation_error"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:src="@mipmap/icon_error" />

                    </RelativeLayout>

                    <TextView
                        android:id="@+id/decoratebudget_365_individuation"
                        style="@style/listview_item_text_style_small"
                        android:layout_height="0dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:paddingBottom="@dimen/space_2"
                        android:text="@string/decoratebudget_365_individuation"
                        android:textColor="@color/font_white" />

                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/space_8"
                    android:src="@mipmap/icon_equal" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@mipmap/bg_req_budget"
                    android:orientation="vertical"
                    android:weightSum="2">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="2">

                        <LinearLayout
                            android:id="@+id/decoratebudget_365_total_price_layout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:orientation="horizontal"
                            android:visibility="gone">

                            <TextView
                                android:id="@+id/decoratebudget_365_total_price"
                                style="@style/style_edit_req_title"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:maxLength="5"
                                android:singleLine="true"
                                android:textColor="@color/black_color"
                                android:textSize="@dimen/big_size"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/decoratebudget_365_total_unit"
                                style="@style/style_edit_req_title"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:text="@string/unit_million"
                                android:textSize="@dimen/small_size" />

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/decoratebudget_365_total_error"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:src="@mipmap/icon_error" />

                    </RelativeLayout>

                    <TextView
                        android:id="@+id/decoratebudget_365_total"
                        style="@style/listview_item_text_style_small"
                        android:layout_height="0dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:paddingBottom="@dimen/space_2"
                        android:text="@string/decoratebudget_365_total"
                        android:textColor="@color/font_white" />


                </LinearLayout>

            </LinearLayout>

            <TextView
                android:id="@+id/act_edit_req_decoratebudget_365_detail"
                style="@style/listview_item_text_style_middle"
                android:layout_gravity="center_horizontal"
                android:drawableLeft="@mipmap/icon_warning"
                android:drawablePadding="@dimen/space_2"
                android:paddingBottom="@dimen/space_8"
                android:paddingTop="@dimen/space_8"
                android:text="@string/decoratebudget_365_detail"
                android:textColor="@color/orange_color" />

        </LinearLayout>

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <View
            style="@style/divier_line"
            android:layout_marginTop="@dimen/space_10"
            android:background="@color/horzontal_line_color" />

        <RelativeLayout
            android:id="@+id/act_edit_req_persons"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <TextView
                android:id="@+id/act_edit_req_persons_title"
                style="@style/style_edit_req_title"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/str_persons" />


            <ImageView
                android:id="@+id/act_edit_req_persons_goto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_next" />

            <TextView
                android:id="@+id/act_edit_req_persons_content"
                style="@style/style_edit_req_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp" />
        </RelativeLayout>

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <RelativeLayout
            android:id="@+id/act_edit_req_lovestyle"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <TextView
                android:id="@+id/act_edit_req_lovestyle_title"
                style="@style/style_edit_req_title"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/str_lovestyle" />


            <ImageView
                android:id="@+id/act_edit_req_lovestyle_goto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_next" />

            <TextView
                android:id="@+id/act_edit_req_lovestyle_content"
                style="@style/style_edit_req_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp" />
        </RelativeLayout>

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <RelativeLayout
            android:id="@+id/act_edit_req_lovedesistyle"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <TextView
                android:id="@+id/act_edit_req_lovedesistyle_title"
                style="@style/style_edit_req_title"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/str_lovedesistyle" />


            <ImageView
                android:id="@+id/act_edit_req_lovedesistyle_goto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_next" />

            <TextView
                android:id="@+id/act_edit_req_lovedesistyle_content"
                style="@style/style_edit_req_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp" />
        </RelativeLayout>

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />

        <RelativeLayout
            android:id="@+id/act_edit_req_lovedesisex"
            style="@style/my_list_item_layout_style"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <TextView
                android:id="@+id/act_edit_req_lovedesisex_title"
                style="@style/style_edit_req_title"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/str_lovedesisex" />

            <ImageView
                android:id="@+id/act_edit_req_lovedesisex_goto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_next" />

            <TextView
                android:id="@+id/act_edit_req_lovedesisex_content"
                style="@style/style_edit_req_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp" />
        </RelativeLayout>

        <View
            style="@style/divier_line"
            android:background="@color/horzontal_line_color" />
    </LinearLayout>

</com.jianfanjia.cn.view.scrollview.ReboundScrollView>
