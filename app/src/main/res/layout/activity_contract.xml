<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/font_white"
    android:orientation="vertical">


    <RelativeLayout
        android:id="@+id/about_head_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_70"
        android:background="@color/head_layout_bg">

        <TextView
            android:id="@+id/share_back"
            style="@style/site_listview_item_text_style_middle"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/space_8"
            android:drawableLeft="@mipmap/icon_register_back"
            android:drawablePadding="4dp"
            android:text="@string/back"
            android:textColor="@color/back_color" />

        <TextView
            android:id="@+id/share_title"
            style="@style/site_listview_item_text_style_title"
            android:layout_centerInParent="true"
            android:text="施工合同"
            android:textColor="@color/main_color_black" />

        <TextView
            android:id="@+id/querenText"
            style="@style/site_listview_item_text_style_middle"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_margin="10dip"
            android:text="确认"
            android:textColor="@color/back_color" />

        <View
            style="@style/divier_line"
            android:layout_alignParentBottom="true"
            android:background="@color/horzontal_line_color" />
    </RelativeLayout>

    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>
