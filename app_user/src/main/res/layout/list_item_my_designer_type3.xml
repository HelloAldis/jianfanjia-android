<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/ltm_req_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/font_white">

    <include
        android:id="@+id/ltm_my_designer_left_layout"
        layout="@layout/include_my_designer_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <View
        android:id="@+id/first_line"
        style="@style/divier_line"
        android:layout_below="@id/ltm_my_designer_left_layout"/>

    <RelativeLayout
        android:id="@+id/ltm_my_designer_content"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_below="@id/first_line">

        <TextView
            android:id="@+id/ltm_my_designer_textview3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textSize="@dimen/middle_size"/>

    </RelativeLayout>


</RelativeLayout>