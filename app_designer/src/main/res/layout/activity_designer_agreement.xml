<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@color/main_layout_bg">

    <com.jianfanjia.cn.designer.view.MainHeadView
        android:id="@+id/my_contract_head_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/head_layout_bg"/>

    <com.jianfanjia.cn.designer.view.ProgressWebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/my_contract_head_layout"/>

    <RelativeLayout
        android:id="@+id/btn_create_process_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/black_light_transparent"
        android:paddingBottom="@dimen/space_8"
        android:paddingLeft="@dimen/space_48"
        android:paddingRight="@dimen/space_48"
        android:paddingTop="@dimen/space_8">

        <TextView
            android:id="@+id/btn_agree"
            android:layout_width="match_parent"
            android:layout_height="@dimen/space_40"
            android:background="@drawable/bg_designer_agreement_rectangle_red"
            android:gravity="center"
            android:text="@string/agree"
            android:textColor="@color/font_white"
            android:textSize="16sp"/>

    </RelativeLayout>

</RelativeLayout>
