        <!-- 友盟配置 -->
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="55e40deee0f55acc66001861"/>
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="Default"/>
        <meta-data
            android:name="SERVER_URL"
            android:value="${SERVER_URL}"/>
        <meta-data
            android:name="MOBILE_SERVER_URL"
            android:value="${MOBILE_SERVER_URL}"/>

        <!-- 个推SDK配置开始 -->
        <!-- 配置的第三方参数属性 -->
        <meta-data
            android:name="PUSH_APPID"
            android:value="${GETUI_PUSH_APPID}"/>
        <meta-data
            android:name="PUSH_APPKEY"
            android:value="${GETUI_PUSH_APPKEY}"/>
        <meta-data
            android:name="PUSH_APPSECRET"
            android:value="${GETUI_PUSH_APPSECRET}"/>
        <meta-data
